<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springquiz.dao.StudentDao">
    <select id="selectList" resultType="Student"> <!--resultType: 알아서 결과물들을 dept객체에 넣어줌-->
        select name, age, java, oracle from student
    </select>
</mapper>